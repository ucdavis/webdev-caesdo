<!doctype html>
<html>
  <head>
    <meta charset="utf-8">

    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">

    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "Green RMI" %></title>

    <%= stylesheet_link_tag "bootstrap.min", "skeleton", "base" %>
    <%= javascript_include_tag  "all", "bootstrap.min" %>
    
    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">
    <link rel="shortcut icon" href="/images/favicon.ico">
    <link href='http://fonts.googleapis.com/css?family=Fira+Sans:300,400,500,700' rel='stylesheet' type='text/css'> <!-- Fira Sans -->
    
  </head>

  <body class="<%= page_classes %>">
    <div class="left_col">
      <div class="left_wrapper">
            <%= partial "header" %>
            <%= yield %>
      </div>  
    </div>
    <div class="right_col">
      test
    </div>
    <%= partial "footer" %>

  </body>
</html>
